name = "file-shuttle"
compatibility_date = "2024-01-01"
pages_build_output_dir = "dist"

# KV Namespace binding (You need to create this in Cloudflare Dashboard)
# id = "YOUR_KV_NAMESPACE_ID"
[[kv_namespaces]]
binding = ""
id = ""

# Environment variables for R2 (Get these from R2 -> Manage R2 API Tokens)
[vars]
R2_ACCOUNT_ID = ""
R2_ACCESS_KEY_ID = ""
R2_SECRET_ACCESS_KEY = ""
R2_BUCKET_NAME = ""
